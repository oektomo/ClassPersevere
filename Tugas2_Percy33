def my_function(fname):
  print(fname + " shaw")

my_function("Eka")
my_function("Mendes")
my_function("Laki") 
Eka shaw
Mendes shaw
Laki shaw

def my_function(fname, lname):
  print(fname + " " + lname)

my_function("Eka", "Setiawan")
Eka Setiawan

def my_function(*kids):
  print("Anak kecil itu adalah " + kids[2])

my_function("Eka", "Tobi", "Lindi")
Anak kecil itu adalah Lindi
Anak kecil itu adalah Lindi

def my_function(child3, child2, child1):
  print("Anak kecil itu adalah " + child3)

my_function(child1 = "Eka", child2 = "Tobi", child3 = "Lindi") 
Anak kecil itu adalah Lindi

def my_function(**kid):
  print("Nama akhir dia adalah " + kid["lname"])

my_function(fname = "eka", lname = "setiawan") 
Nama akhir dia adalah setiawan

def my_function(country = "Indonesia"):
  print("Saya dari " + country)

my_function("Sweden")
my_function("India")
my_function()
my_function("Brazil") 
Saya dari Sweden
Saya dari India
Saya dari Indonesia
Saya dari Brazil

def my_function(food):
  for x in food:
    print(x)

fruits = ["apple", "banana", "cherry"]

my_function(fruits)
